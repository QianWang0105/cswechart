<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.system.code">
	<select id="getCodetypes" parameterType="java.util.Map" resultType="java.util.Map"  >
		select t.codetype,t.key,t.value,t.maintypedescribe,t.maintype from t_s_code t
		order by t.codetype,t.orderby
	</select>
</mapper>